  border-radius: 100%;
  background-color: #F5F5F5;
  text-align: center;
  position: relative;
}
.num-btn.num-decr:before {
  content: '';
  display: block;
  width: 20rpx;
  height: 4rpx;
  background-color: #aaa;
  border-radius: 2rpx;
  position: absolute;
  left: 12rpx;
  top: 20rpx;
}
.num-btn.num-incr:before {
  content: '';
  display: block;
  width: 20rpx;
  height: 4rpx;
  background-color: #aaa;
  border-radius: 2rpx;
  position: absolute;
  left: 12rpx;
  top: 20rpx;
}
.num-btn.num-incr:after {
  content: '';
  display: block;
  width: 4rpx;
  height: 20rpx;
  background-color: #aaa;
  border-radius: 2rpx;
  position: absolute;
  left: 20rpx;
  top: 12rpx;
}
